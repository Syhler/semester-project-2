<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.presentation.dialogControllers.ImportController">
   <children>
      <Label text="Importing programs...">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <ProgressBar prefWidth="200.0" progress="0.0" fx:id="progress_bar">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </ProgressBar>
      <Label fx:id="statusLabel" alignment="CENTER" prefHeight="10.0" prefWidth="436.0" text="1/38 imported..." textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#okOnAction" text="Ok" fx:id="okButton" disable="true">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
