<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" prefHeight="566.0" prefWidth="400.0" spacing="15.0" stylesheets="@css/LoginPageStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.presentation.AuthenticationController">
   <children>
      <Label prefHeight="82.0" prefWidth="214.0" text="Create XXXXXXXXX" />
      <Label text="First name:*" />
      <TextField />
      <Label text="Middle name:" />
      <TextField />
      <Label text="Last name:*" />
      <TextField />
      <Label text="Email:*" />
      <TextField />
      <Label text="Company:*" />
      <TextField />
      <Label text="Role:*" />
      <TextField />
      <Label text="Title:*" />
      <TextField />
      <VBox alignment="CENTER" pickOnBounds="false" prefHeight="67.0" prefWidth="200.0">
         <children>
            <HBox>
               <children>
                  <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="70.0" text="Create" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnCancel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0" prefWidth="70.0" text="Cancel" />
               </children>
               <VBox.margin>
                  <Insets left="15.0" right="15.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="40.0" left="100.0" right="100.0" top="50.0" />
   </padding>
</VBox>
