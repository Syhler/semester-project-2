<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.presentation.DeleteController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="100.0">
         <children>
            <Label fx:id="deleteWarningText" alignment="CENTER" text="You sure you want to delete?" wrapText="true">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0">
         <children>
            <Button fx:id="yesBtn" mnemonicParsing="false" onAction="#deleteProgramYes" text="Yes">
               <HBox.margin>
                  <Insets left="5.0" right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="noBtn" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#deleteProgramNo" text="No">
               <HBox.margin>
                  <Insets left="20.0" right="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
