<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="program" prefHeight="220.0" style="-fx-border-style: none none solid none; -fx-border-width: 2; -fx-border-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.presentation.ProgramController">
   <children>
      <VBox fx:id="programThumbnail" alignment="CENTER">
         <children>
            <ImageView fx:id="programImage" fitHeight="150.0" fitWidth="240.0" pickOnBounds="true">
               <image>
                  <Image url="@loginImages/loginUserIcon.png" />
               </image>
            </ImageView>
            <Label fx:id="title" alignment="TOP_LEFT" layoutX="10.0" layoutY="10.0" text="Title" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </children>
</VBox>
